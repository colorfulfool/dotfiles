[
  {
    "context": "Pane",
    "bindings": {
      "cmd-1": ["pane::ActivateItem", 0],
      "cmd-2": ["pane::ActivateItem", 1],
      "cmd-3": ["pane::ActivateItem", 2],
      "cmd-4": ["pane::ActivateItem", 3],
      "cmd-5": ["pane::ActivateItem", 4],
      "cmd-6": ["pane::ActivateItem", 5],
      "cmd-7": ["pane::ActivateItem", 6],
      "cmd-8": ["pane::ActivateItem", 7],
      "cmd-9": ["pane::ActivateItem", 8],
      "cmd-ctrl-w": "pane::CloseInactiveItems"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "cmd-shift-k": "editor::DeleteLine",
      "cmd-shift-up": "editor::MoveLineUp",
      "cmd-shift-down": "editor::MoveLineDown",
      "cmd-shift-d": "editor::DuplicateLineDown",
      "alt-h": null
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "cmd-shift-r": "project_symbols::Toggle",
      "cmd-shift-t": "task::Spawn",
      "alt-h": ["workspace::ActivatePaneInDirection", "Left"],
      "alt-l": ["workspace::ActivatePaneInDirection", "Right"],
      "alt-t": "task::Spawn"
    }
  },
  {
    "context": "Editor && mode == full",
    "bindings": {
      "cmd-t": "outline::Toggle"
    }
  }
]
